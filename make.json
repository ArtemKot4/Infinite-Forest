{
	"$schema": "https://raw.githubusercontent.com/zheka2304/innercore-mod-toolchain/develop/toolchain/schemas/make.schema.json",
	"resources": [
		{
			"type": "minecraft_resource_pack",
			"path": "minecraft_packs/infinite_forest_resources"
		},
	    {
		"type": "minecraft_behavior_pack",
		"path": "minecraft_packs/infinite_forest_behavior"
		},
		{
			"path": "assets",
			"type": "resource_directory"
		},
		{
			"path": "structures",
			"type": "resource_directory"
		},
		{
			"path": "models",
			"type": "resource_directory"
		},
		{
			"path": "assets/gui",
			"type": "gui"
		}
	],
	"sources": [
		{
			"source": "code",
			"target": "main.js",
			"type": "main",
			"language": "typescript"
		},
		{
			"source": "launcher.js",
			"type": "launcher",
			"language": "javascript"
		},
		{
			"source": "lib/*",
			"type": "library",
			"language": "javascript"
		}
	],
	"compile": [
		{
			"source": "native",
			"type": "native"
		},
		{
			"source": "java",
			"type": "java"
		}
	],
	"additional": [
		{
			"source": "config.json",
			"targetDir": "."
		},
		{
			"source": "config.info.json",
			"targetDir": "."
		}
	],
	"info": {
		"name": "Infinite Forest",
		"author": "Artem_",
		"version": "last",
		"description": "Fire is burns",
		"icon": "mod_icon.png"
	},
	"api": "CoreEngine",
	"linkNative": [
		"minecraftpe",
		"innercore",
		"nativejs"
	],
	"excludeFromRelease": []
}




















